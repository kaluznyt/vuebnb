{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    
    "target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": ["webpack-env"]
  },
  "exclude": [
      "bin",
      "node_modules",
      "wwwroot"
  ]
}
